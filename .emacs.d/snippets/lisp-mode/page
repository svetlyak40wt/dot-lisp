# -*- mode: snippet -*-
# name: page
# key: page
# --
(uiop:define-package #:${1:system-name}/${2:widget-name}
  (:use #:cl)
  (:import-from #:reblocks/widget
                #:render
                #:defwidget)
  (:import-from #:reblocks/html
                #:with-html))
(in-package #:$1/$2)


(defwidget ${3:$2-page} ()
  ())


(defun make-$3 ()
  (make-instance '$3))


(defmethod render ((widget $3))
  (with-html
    (:p "Fill me with code")))
